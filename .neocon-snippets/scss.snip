snippet cf
  .cf {
    zoom: 1;
  }
  .cf:after {
    clear: both;
    content: '';
    display: block;
    height: 0;
    overflow: hidden;
    visibility: hidden;
  }

snippet reset
  * {
    margin: 0;
    padding: 0;
    list-style: none;
    box-sizing: border-box;
  }

snippet fontfamily
alias ff
  'ヒラギノ角ゴ Pro W3', 'Hiragino Kaku Gothic Pro', 'メイリオ', Meiryo, 'ＭＳ Ｐゴシック', 'MS PGothic', Helvetica, Arial, sans-serif

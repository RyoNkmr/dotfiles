snippet viewport
alias vp
  <meta name="viewport" content="initial-scale: 1.0">

snippet extend
alias ex
  <% function ${1: test} (buf) { %>
    ${2}
  <% }; %>

snippet input
alias in
  <input type="${1}"${2}>${3}

snippet comment
alias cm
  <%#${1}%>

snippet todo
alias td
  <%#TODO(thrbrd) ${1}%>

snippet note
alias nt
  <%#NOTE(thrbrd) ${1}%>
